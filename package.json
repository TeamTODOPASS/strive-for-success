    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "null-loader": "^0.1.1",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "~2.5.4",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "1.1.2",
