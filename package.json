{
  "name": "strive-for-success",
  "version": "1.0.0",
  "description": "Telerik Software Academy - Angular 2 Team Project",
  "scripts": {
    "start": "npm run build:prod && node server/server.js",
    "dev": "concurrently \"npm run build:dev\" \"nodemon server/server.js\"",
    "test": "karma start",
    "server": "node server/server.js",
    "client": "webpack-dev-server --config config/webpack.dev.js --progress --inline --profile --watch --content-base client/app/ --port 3000",
    "build:dev": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile",
    "build:prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamTODOPASS/strive-for-success.git"
  },
  "author": "TeamTODOPASS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamTODOPASS/strive-for-success/issues"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "core-js": "^2.4.1",
    "crypto": "0.0.3",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "mongodb": "^2.2.11",
    "mongoose": "^4.7.0",
    "mongoose-unique-validator": "^1.0.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "@types/webpack": "^1.12.34",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.1",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
